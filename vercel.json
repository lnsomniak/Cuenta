{
"version": 2,
"builds": [
    {
    "src": "api/server.py",
    "use": "@vercel/python"
    },
    {
    "src": "web/package.json",
    "use": "@vercel/next"
    }
],
"routes": [
    {
    "src": "/api/(.*)",
    "dest": "api/server.py"
    },
    {
    "src": "/(.*)",
    "dest": "web/$1"
    }
],
"installCommand": "pip install -r api/requirements.txt && npm install --prefix web"
}