{
"version": 2,
"builds": [
    {
    "src": "api/server.py",
    "use": "@vercel/python",
    "config": {
        "pipInstallCommand": "pip install -r requirements.txt"
    }
    },
    {
    "src": "web/package.json",
    "use": "@vercel/next"
    }
],
"routes": [
    {
    "src": "/api/(.*)",
    "dest": "api/server.py"
    },
    {
    "src": "/(.*)",
    "dest": "web/$1"
    }
    ]
}